<p id="notice"><%= notice%> </p>

<h1>Meetings</h1>

<table class = "table table-sm table-striped">
  <thead>
    <tr>
      <th scope = "co1">Name</th>
      <th scope = "co1">Start time</th>
    </tr>
  </thead>
  </tbody>
    <% @plans.each do |plan|%>
      <tr>
        <td><%= plan.name%></td>
         <td><%= plan.start_time%></td>
      </tr>
    <%end%>
  </tbody>
</table>

<%= month_calendar events:@plans do |date,plans|%>
  <%= date %>

  <% plans.each do |plan|%>
    <%= plan.name%>
  <%end%>
<% end %>